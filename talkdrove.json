{
  "name": "MAHII-MD",
  "description": "Powerful WhatsApp bot made by Mihiranga using Baileys",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js"
  },
  "keywords": ["whatsapp", "bot", "baileys", "MAHII-MD"],
  "author": "Mihiranga",
  "env": {
    "SESSION_ID": {
      "description": "Your WhatsApp session ID",
      "required": true
    },
    "OWNER_NUM": {
      "description": "Your WhatsApp number",
      "required": true
    },
    "PREFIX": {
      "description": "Bot command prefix",
      "required": false
    },
    "AUTO_READ_STATUS": {
      "description": "Enable auto read status (true/false)",
      "required": false
    },
    "AUTO_REACT_STATUS": {
      "description": "Enable auto react to status (true/false)",
      "required": false
    },
    "AUTO_RECORDING": {
      "description": "Bot appears as recording (true/false)",
      "required": false
    },
    "AUTO_STATUS_LIKE": {
      "description": "Auto-like status feature",
      "required": false
    },
    "Mode": {
      "description": "Bot mode: public or private",
      "required": true
    }
  }
}
